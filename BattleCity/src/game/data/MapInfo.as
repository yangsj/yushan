		/**
		 * 获取关卡地图数据
		 * @param level
		 * @return 
		 * 
		 */
		public static function getInfoByLevel( level:int ):Array
		{
			var map:Array = MapInfo[ "MAP_" + level ];
			if ( map == null )
			{
				throw new Error("找不到相关配置！");
			}
			var result:Array = [];
			for each ( var arr:Array in map )
			{
				var temp:Array = [];
				for each ( var obj:* in arr )
				{
					temp.push( obj );
				}
				result.push( temp );
			}
			return result;
		}
		
